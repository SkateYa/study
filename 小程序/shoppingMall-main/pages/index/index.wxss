/* 轮播图 */
.index-swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index-swiper swiper image {
  width: 100%;
}
/* 楼层 */
.index-cate {
  display: flex;
}
.index-cate .cate-item {
  flex: 1;
  padding: 20rpx;
}
.index-cate .cate-item image {
  width: 100%;
}
/* 下面列表 */
.index-floor .floor-group .floor-title {
  padding: 10rpx 0;
}
.index-floor .floor-group .floor-title image {
  width: 100%;
}
.index-floor .floor-list {
  overflow: hidden;
}
.index-floor .floor-list navigator {
  float: left;
  width: 33.33%;
  /* 后面4个超链接 */
}
.index-floor .floor-list navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index-floor .floor-list navigator:nth-child(2),
.index-floor .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index-floor .floor-list navigator image {
  width: 100%;
  height: 100%;
}
